{
    "compilerOptions": {
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "moduleResolution": "node",
        "noImplicitAny": true,
        "outDir": "./dist",
        "resolveJsonModule": true,
        "sourceMap": true,
        "declaration": true,
        "declarationMap": true,
        "strict": true,
        "module": "commonjs",
        "target": "ES5"
    },
    "exclude": [
        "node_modules"
    ],
    "include": [
        "src"
    ],
}